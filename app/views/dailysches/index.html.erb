<p id="notice"><%= notice %></p>

<h1>Dailysches</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Counsellor</th>
      <th>Time</th>
      <th>Client</th>
      <th>Ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailysches.each do |dailysch| %>
      <tr>
        <td><%= dailysch.date %></td>
        <td><%= dailysch.counsellor %></td>
        <td><%= dailysch.time %></td>
        <td><%= dailysch.client %></td>
        <td><%= dailysch.ref %></td>
        <td><%= link_to 'Show', dailysch %></td>
        <td><%= link_to 'Edit', edit_dailysch_path(dailysch) %></td>
        <td><%= link_to 'Destroy', dailysch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dailysch', new_dailysch_path %>
